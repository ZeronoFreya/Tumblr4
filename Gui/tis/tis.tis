function appendImgLoading( html ){
    self.$(#ul).append(html);
}
function setImgId( id, i ){
    // stdout.println(id, i);
    var li = $(li.loading[imgid="{i}"]);
    if(li){
        li.@#imgid = id;
    }
    // li.@#imgid = id;
    // li.@.removeClass("loading");
}
function setImgBg( id, path ){
    // stdout.println(id, path);
    var li = $(li[imgid="{id}"]);
    // stdout.println(li);
    if(li){
        li.text = id;
        li.post( ::this.style#background-image = "url(" + path + ")" );
        li.@.removeClass("loading");
    }

}